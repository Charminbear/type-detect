{
  "name": "type-detect",
  "version": "1.0.0",
  "description": "Improved typeof detection for node.js and the browser.",
  "keywords": [],
  "license": "MIT",
  "author": "Jake Luer <jake@alogicalparadox.com> (http://alogicalparadox.com)",
  "main": "./index",
  "repository": {
    "type": "git",
    "url": "git@github.com:chaijs/type-detect.git"
  },
  "scripts": {
    "build": "browserify --bare $npm_pakcage_main --standalone typeDetect -o type-detect.js",
    "lint": "eslint --ignore-path .gitignore .",
    "prepublish": "npm run build",
    "pretest": "echo disabled for now; exit 0; npm run lint",
    "test": "npm run test:node-cover && npm run test:browser",
    "test:browser": "karma start --singleRun=true",
    "test:node": "mocha",
    "test:node-cover": "mocha"
  },
  "eslintConfig": {
    "extends": [
      "strict/es5"
    ],
    "rules": {
      "complexity": 0
    }
  },
  "dependencies": {},
  "devDependencies": {
    "browserify": "^13.0.0",
    "component": "*",
    "coveralls": "2.11.8",
    "eslint": "^2.4.0",
    "eslint-config-strict": "^8.5.0",
    "eslint-plugin-filenames": "^0.2.0",
    "jscoverage": "0.3.7",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.2",
    "karma-coverage": "^0.5.5",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sauce-launcher": "^0.3.1",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.5",
    "simple-assert": "*"
  },
  "engines": {
    "node": "*"
  }
